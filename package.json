{
  "name": "mavenmm-tw-netlify-react-sso",
  "version": "1.0.0",
  "description": "Maven Marketing SSO system for React apps with Teamwork authentication and Netlify Functions",
  "type": "module",
  "main": "dist/index.esm.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.esm.js",
      "types": "./dist/index.d.ts",
      "default": "./dist/index.esm.js"
    },
    "./server": {
      "import": "./dist/server.esm.js",
      "types": "./dist/server.d.ts",
      "default": "./dist/server.esm.js"
    },
    "./netlify": {
      "types": "./dist/netlify/createHandlers.d.ts",
      "import": "./dist/netlify/createHandlers.js",
      "default": "./dist/netlify/createHandlers.js"
    }
  },
  "bin": {
    "mavenmm-scaffold": "./dist/cli/scaffold.js"
  },
  "files": [
    "dist",
    "netlify",
    "README.md",
    "CHANGELOG.md"
  ],
  "scripts": {
    "build": "rollup -c",
    "build:watch": "rollup -c -w",
    "dev": "npm run build:watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:netlify": "node test-netlify.js",
    "type-check": "tsc --noEmit",
    "prepare": "npm run build",
    "prepublishOnly": "npm run type-check && npm run build"
  },
  "keywords": [
    "maven-marketing",
    "sso",
    "authentication",
    "react",
    "netlify",
    "teamwork",
    "oauth",
    "typescript"
  ],
  "author": "Maven Marketing",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/your-username/mavenmm-tw-netlify-react-sso.git"
  },
  "bugs": {
    "url": "https://github.com/your-username/mavenmm-tw-netlify-react-sso/issues"
  },
  "homepage": "https://github.com/your-username/mavenmm-tw-netlify-react-sso#readme",
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0",
    "react-router-dom": ">=6.0.0"
  },
  "devDependencies": {
    "@netlify/functions": "^2.0.0",
    "@rollup/plugin-commonjs": "^25.0.0",
    "@rollup/plugin-node-resolve": "^15.2.0",
    "@rollup/plugin-typescript": "^11.1.0",
    "@testing-library/jest-dom": "^6.0.0",
    "@testing-library/react": "^14.0.0",
    "@types/cookie": "^0.6.0",
    "@types/jest": "^29.5.0",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/node": "^20.0.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "jest": "^29.6.0",
    "jest-environment-jsdom": "^29.6.0",
    "rollup": "^3.28.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "ts-jest": "^29.1.0",
    "tslib": "^2.6.0",
    "typescript": "^5.1.0",
    "vite": "^6.3.5"
  },
  "dependencies": {
    "@teamwork/login-button": "^1.0.1",
    "cookie": "^0.6.0",
    "jsonwebtoken": "^9.0.2",
    "react-router-dom": ">=6.0.0"
  }
}
