{
  "name": "@mavenmm/teamwork-auth",
  "version": "2.0.5",
  "description": "Centralized Teamwork SSO authentication system for React applications with zero backend code required",
  "type": "module",
  "main": "dist/index.esm.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.esm.js",
      "types": "./dist/index.d.ts",
      "default": "./dist/index.esm.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "CHANGELOG.md",
    "CLAUDE.md"
  ],
  "scripts": {
    "build": "rollup -c",
    "build:watch": "rollup -c -w",
    "dev": "npm run build:watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "type-check": "tsc --noEmit",
    "prepare": "npm run build",
    "prepublishOnly": "npm run type-check && npm run build"
  },
  "keywords": [
    "teamwork",
    "sso",
    "authentication",
    "react",
    "netlify",
    "oauth",
    "typescript"
  ],
  "author": "",
  "license": "ISC",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mavenmm/teamwork-auth.git"
  },
  "bugs": {
    "url": "https://github.com/mavenmm/teamwork-auth/issues"
  },
  "homepage": "https://github.com/mavenmm/teamwork-auth#readme",
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0",
    "react-router-dom": ">=6.0.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.0",
    "@rollup/plugin-node-resolve": "^15.2.0",
    "@rollup/plugin-typescript": "^11.1.0",
    "@testing-library/jest-dom": "^6.0.0",
    "@testing-library/react": "^14.0.0",
    "@types/jest": "^29.5.0",
    "@types/node": "^20.0.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "concurrently": "^8.2.0",
    "jest": "^29.6.0",
    "jest-environment-jsdom": "^29.6.0",
    "rollup": "^3.28.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "ts-jest": "^29.1.0",
    "tslib": "^2.6.0",
    "typescript": "^5.1.0"
  },
  "dependencies": {
    "@teamwork/login-button": "^1.0.1",
    "buffer": "^6.0.0",
    "process": "^0.11.10"
  }
}
